    "path": "^0.12.7",
    "stream": "^0.0.2",
    "string_decoder": "^1.3.0",
    "url": "^0.11.1"
  },
  "devDependencies": {
    "@bundled-es-modules/memfs": "^4.2.3",
    "esbuild": "^0.18.10"
  }
