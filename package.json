{
  "name": "@bundled-es-modules/glob",
  "version": "10.3.5",
  "description": "mirror of glob, bundled and exposed as ES module",
  "author": "Joren Broekema <joren.broekema@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "require": "./index-cjs.cjs",
      "node": "./index-esm-node.js",
      "default": "./index-esm.js"
    }
  },
  "scripts": {
    "postinstall": "patch-package",
    "test": "run-p test:*",
    "test:browser": "wds --open --node-resolve",
    "test:node": "node dev-node.js",
    "test:node:custom": "node dev-node-custom.js",
    "build": "run-p build:*",
    "build:browser": "esbuild source.js --bundle --format=esm --outfile=index-esm.js && node manual-adjust-esm.js",
    "build:node": "esbuild source.js --bundle --format=esm --platform=node --outfile=index-esm-node.js"
  },
  "keywords": [],
  "files": [
    "index-esm.js",
    "index-esm-node.js",
    "index-cjs.cjs"
  ],
  "license": "ISC",
  "dependencies": {
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "glob": "^10.3.3",
    "patch-package": "^8.0.0",
    "path": "^0.12.7",
    "stream": "^0.0.2",
    "string_decoder": "^1.3.0",
    "url": "^0.11.1"
  },
  "devDependencies": {
    "@bundled-es-modules/memfs": "^4.2.3",
    "@web/dev-server": "^0.3.3",
    "esbuild": "^0.18.10",
    "npm-run-all": "^4.1.5"
  }
}
